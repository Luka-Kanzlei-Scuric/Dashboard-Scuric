# N8N ClickUp Integration Environment Variables
# Copy this file to .env in the backend directory

# N8N Configuration
N8N_WEBHOOK_URL=https://n8n.scuric.de/webhook

# ClickUp List IDs
CLICKUP_LEADS_LIST_ID=your_leads_list_id
CLICKUP_OFFERS_LIST_ID=your_offers_list_id

# Server Configuration
PORT=5001
NODE_ENV=development

# MongoDB Connection (required for production)
MONGODB_URI=mongodb+srv://username:password@cluster.mongodb.net/database

# Frontend URL for redirects
FRONTEND_URL=http://localhost:5173