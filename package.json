{
    "name": "scuric-dashboard",
    "version": "1.0.0",
    "description": "Scuric Dashboard with integrated backend",
    "main": "src/server/server.js",
    "scripts": {
        "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
        "dev:server": "nodemon src/server/server.js",
        "dev:client": "vite src/client",
        "build": "vite build src/client",
        "start": "node src/server/server.js",
        "lint": "eslint ."
    },
    "dependencies": {
        "express": "^4.18.2",
        "mongoose": "^8.1.1",
        "cors": "^2.8.5",
        "body-parser": "^1.20.2",
        "dotenv": "^16.4.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-router-dom": "^6.22.0",
        "@mui/material": "^5.15.7",
        "@emotion/react": "^11.11.3",
        "@emotion/styled": "^11.11.0"
    },
    "devDependencies": {
        "@vitejs/plugin-react": "^4.2.1",
        "concurrently": "^8.2.2",
        "nodemon": "^3.0.3",
        "vite": "^5.0.12",
        "eslint": "^8.56.0",
        "eslint-plugin-react": "^7.33.2"
    }
}
